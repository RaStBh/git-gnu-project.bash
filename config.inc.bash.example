## GNU standard readme files.
##
## Example:
##
##   readme_files=();
##   readme_files[0]='AUTHORS';
##   readme_files[1]='BACKLOG'
##   readme_files[2]='BUGS';
##   readme_files[3]='CHANGELOG';
##   readme_files[4]='CONTRIBUTING';
##   readme_files[5]='COPYING';
##   readme_files[6]='FAQ';
##   readme_files[7]='HACKING';
##   readme_files[8]='INSTALL';
##   readme_files[9]='NEWS';
##   readme_files[10]='README';
##   readme_files[11]='THANKS';
##   readme_files[12]='TODO';
##
##   or
##
##   readme_files=(
##     'AUTHORS' \
##     'BACKLOG' \
##     'BUGS' \
##     'CHANGELOG' \
##     'CONTRIBUTING' \
##     'COPYING' \
##     'FAQ' \
##     'HACKING' \
##     'INSTALL' \
##     'NEWS' \
##     'README' \
##     'THANKS' \
##     'TODO' \
##   );

readme_files=(
  'AUTHORS' \
  'BACKLOG' \
  'BUGS' \
  'CHANGELOG' \
  'CONTRIBUTING' \
  'COPYING' \
  'FAQ' \
  'HACKING' \
  'INSTALL' \
  'NEWS' \
  'README' \
  'THANKS' \
  'TODO' \
);

## Package project tree layout - the type of the package.
##
##   program  - prg
##   library  - lib
##   document - doc
##   package  - pkg
##
## Example:
##
##   type='pkg';

type='pkg';

## Package project tree layout - The Pitchfork Layout (PFL)
##
##   build/
##     subbuild1/
##     subbuild2/
##   data/
##   docs/
##   examples/
##   external/
##     subexternal1/
##     subexternal2/
##   extras/
##     data/
##     docs/
##     examples/
##     include/
##     src/
##     tests/
##   include/
##   libs/
##     data/
##     docs/
##     examples/
##     include/
##     src/
##     tests/
##   src/
##   tests/
##   tools/
##
## Git only recognise files  but not directories.  So we add  a .gitkeep to each
## directory so Git will not delete the directory.

## Package project tree - common directories and files for all project types.
##
## Example:
##
##   common_tree=();
##
##   or
##
##   common_tree=( \
##   );

common_tree=( \
);

## Package project tree - special directories  and files for program source code
## (with documentation) type of project.
##
## Example:
##
##   pogram_tree=();
##   pogram_tree[0]='build/.gitkeep';
##   pogram_tree[1]='data/.gitkeep';
##   pogram_tree[2]='docs/.gitkeep';
##   pogram_tree[3]='examples/.gitkeep';
##   pogram_tree[4]='external/.gitkeep';
##   pogram_tree[5]='extras/.gitkeep';
##   pogram_tree[6]='include/.gitkeep';
##   pogram_tree[7]='libs/.gitkeep';
##   pogram_tree[8]='src/.gitkeep';
##   pogram_tree[9]='tests/.gitkeep';
##   pogram_tree[10]='tools/.gitkeep';
##
##   or
##
##   pogram_tree=(
##     'build/.gitkeep'
##     'data/.gitkeep'
##     'docs/.gitkeep'
##     'examples/.gitkeep'
##     'external/.gitkeep'
##     'extras/.gitkeep'
##     'include/.gitkeep'
##     'libs/.gitkeep'
##     'src/.gitkeep'
##     'tests/.gitkeep'
##     'tools/.gitkeep'
##   );

pogram_tree=( \
  'build/.gitkeep' \
  'data/.gitkeep' \
  'docs/.gitkeep' \
  'examples/.gitkeep' \
  'external/.gitkeep' \
  'extras/.gitkeep' \
  'include/.gitkeep' \
  'libs/.gitkeep' \
  'src/.gitkeep' \
  'tests/.gitkeep' \
  'tools/.gitkeep' \
);

## Package project tree - special directories  and files for library source code
## (with documentation) type of project.
##
## Example:
##
##   library_tree=();
##   library_tree[0]='build/.gitkeep';
##   library_tree[1]='data/.gitkeep';
