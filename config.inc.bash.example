##   library_tree[8]='src/.gitkeep';
##   library_tree[9]='tests/.gitkeep';
##   library_tree[10]='tools/.gitkeep';
##
##   or
##
##   library_tree=(
##     'build/.gitkeep'
##     'data/.gitkeep'
##     'docs/.gitkeep'
##     'examples/.gitkeep'
##     'external/.gitkeep'
##     'extras/.gitkeep'
##     'include/.gitkeep'
##     'libs/.gitkeep'
##     'src/.gitkeep'
##     'tests/.gitkeep'
##     'tools/.gitkeep'
##   );

library_tree=( \
  'build/.gitkeep' \
  'data/.gitkeep' \
  'docs/.gitkeep' \
  'examples/.gitkeep' \
  'external/.gitkeep' \
  'extras/.gitkeep' \
  'include/.gitkeep' \
  'libs/.gitkeep' \
  'src/.gitkeep' \
  'tests/.gitkeep' \
  'tools/.gitkeep' \
);

